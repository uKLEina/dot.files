! disable keycap picture insert mode
urxvt.iso14755: false
urxvt.iso14755_52: false

urxvt*geometry: 96x32
urxvt*scrollBar: false
! urxvt*scrollBar_right: true
! urxvt*scrollColor: #323232
urxvt*cursorColor: #eeeeee
!urxvt*underLine: on
URxvt.pointerBlank: true
URxvt.visualBell: false
URxvt.saveLines: 100000

! color
URxvt.depth: 32
URxvt.fading: 40
urxvt*colorMode: on
urxvt*colorBDMode: on
urxvt*colorBD: #f2a22b
urxvt*colorULMode: on
urxvt*colorUL: #7fa9e8
urxvt*colorITMode: on
urxvt*colorIT: #40eb84
urxvt*dynamicColors: off
urxvt*highlightSelection: true

! colorscheme
! Dracula Xresources palette
! slightly modified to lighter color 8 for transparent background
*.foreground: #F8F8F2
*.background: [90]#282A36
*.color0:     #000000
*.color1:     #FF5555
*.color2:     #50FA7B
*.color3:     #F1FA8C
*.color4:     #BD93F9
*.color5:     #FF79C6
*.color6:     #8BE9FD
*.color7:     #BFBFBF
*.color8:     #666666
*.color9:     #FF6E67
*.color10:    #5AF78E
*.color11:    #F4F99D
*.color12:    #CAA9FA
*.color13:    #FF92D0
*.color14:    #9AEDFE
*.color15:    #E6E6E6

! font
urxvt*font: xft:Ricty Discord:pixelsize=32:antialias=true:hinting=true
urxvt*boldFont: xft:Ricty Discord:pixelsize=32:antialias=true:bold:hinting=true
urxvt*italicFont: xft:Ricty Discord:pixelsize=32:antialias=true:italic:autohint=true:hinting=true
urxvt*bolditalicFont: xft:Ricty Discord:pixelsize=32:antialias=true:bold:italic:autohint=true:hinting=true
urxvt*letterSpace: -1

urxvt*urlLauncher: /usr/bin/firefox

Xft.dpi: 144
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
